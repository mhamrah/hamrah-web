{
  "name": "my-qwik-basic-starter",
  "description": "Demo app with sample routes",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "pnpm run build && wrangler dev",
    "serve": "wrangler pages dev ./dist --compatibility-flags=nodejs_als",
    "start": "vite --open --mode ssr",
    "cf-typegen": "wrangler types",
    "prepare": "pnpm dlx husky install",
    "qwik": "qwik",
    "db:generate": "pnpm dlx drizzle-kit generate",
    "db:push": "pnpm dlx drizzle-kit push",
    "db:migrate": "pnpm wrangler d1 migrations apply prod-hamrah-app-auth",
    "db:migrate:remote": "pnpm wrangler d1 migrations apply prod-hamrah-app-auth --remote",
    "db:status": "pnpm wrangler d1 migrations list prod-hamrah-app-auth",
    "db:status:remote": "pnpm wrangler d1 migrations list prod-hamrah-app-auth --remote",
    "db:studio": "pnpm dlx drizzle-kit studio"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.15.0",
    "@builder.io/qwik-city": "^1.15.0",
    "@cloudflare/workers-types": "^4.20250816.0",
    "@eslint/js": "^9.33.0",
    "@qwik-ui/headless": "0.7.0",
    "@qwik-ui/styled": "0.3.4",
    "@qwik-ui/utils": "0.3.2",
    "@qwikest/icons": "^0.0.13",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "24.3.0",
    "@types/oidc-provider": "9.1.2",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "class-variance-authority": "^0.7.1",
    "drizzle-kit": "0.31.4",
    "eslint": "9.33.0",
    "eslint-plugin-qwik": "^1.15.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "nx": "21.4.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "qwik-ui": "0.3.1",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.1",
    "undici": "^7.14.0",
    "vite": "^7.1.2",
    "vite-plugin-mkcert": "1.17.8",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.30.0"
  },
  "nx": {},
  "dependencies": {
    "@oslojs/crypto": "1.0.1",
    "@oslojs/encoding": "1.1.0",
    "@simplewebauthn/browser": "13.1.2",
    "@simplewebauthn/server": "13.1.2",
    "arctic": "3.7.0",
    "drizzle-orm": "0.44.4",
    "jose": "6.0.12",
    "oidc-provider": "9.4.1"
  }
}